model FRED_GDP {
  DateTime DateTime @id(map: "PK_FRED_GDP") @db.DateTime
  USA      Decimal? @db.Decimal(20, 3)
  India    Decimal? @db.Decimal(20, 3)
  China    Decimal? @db.Decimal(20, 3)
  Japan    Decimal? @db.Decimal(20, 3)
  Germany  Decimal? @db.Decimal(20, 3)
  UK       Decimal? @db.Decimal(20, 3)
  SK       Decimal? @db.Decimal(20, 3)
  Canada   Decimal? @db.Decimal(20, 3)
}

