generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model COMMODITY {
  Symbol   String   @db.VarChar(50)
  DateTime DateTime @db.DateTime
  Rate     Decimal? @db.Decimal(18, 2)

  @@id([Symbol, DateTime], map: "PK_COMMODITY_SPY")
}

model CPI {
  DateTime         DateTime @id(map: "PK_CPI") @db.DateTime
  CPIU19821984     Decimal? @db.Decimal(10, 5)
  CPIUNoFoodEnergy Decimal? @db.Decimal(10, 5)
}

model CPI_12_Month_Percent_Change {
  DateTime                     DateTime @id(map: "PK_CPI_12_Month_Percent_Change") @db.DateTime
  AllItems                     Decimal? @db.Decimal(7, 2)
  FoodAtHome                   Decimal? @db.Decimal(7, 2)
  Energy                       Decimal? @db.Decimal(7, 2)
  Electricity                  Decimal? @db.Decimal(7, 2)
  AllItemsLessFoodAndEnergy    Decimal? @db.Decimal(7, 2)
  Apparel                      Decimal? @db.Decimal(7, 2)
  MedicalCareCommodities       Decimal? @db.Decimal(7, 2)
  Shelter                      Decimal? @db.Decimal(7, 2)
  EducationAndCommunication    Decimal? @db.Decimal(7, 2)
  Food                         Decimal? @db.Decimal(7, 2)
  FoodAwayFromHome             Decimal? @db.Decimal(7, 2)
  GasolineAllTypes             Decimal? @db.Decimal(7, 2)
  NaturalGasPiped              Decimal? @db.Decimal(7, 2)
  CommoditiesLessFoodAndEnergy Decimal? @db.Decimal(7, 2)
  NewVehicles                  Decimal? @db.Decimal(7, 2)
  ServicesLessEnergyServices   Decimal? @db.Decimal(7, 2)
  MedicalCareServices          Decimal? @db.Decimal(7, 2)
}

model CPI_Average_Price {
  DateTime                         DateTime @id(map: "PK_CPI_Average_Price") @db.DateTime
  BananasPerLb                     Decimal? @db.Decimal(6, 3)
  BreadWhitePanPerLb               Decimal? @db.Decimal(6, 3)
  ChickenFreshWholePerLb           Decimal? @db.Decimal(6, 3)
  EggsGradeALargePerDozen          Decimal? @db.Decimal(6, 3)
  GroundChuckAllBeefPerLb          Decimal? @db.Decimal(6, 3)
  MilkFreshWholeFortifiedPerGallon Decimal? @db.Decimal(6, 3)
  OrangesNavelPerLb                Decimal? @db.Decimal(6, 3)
  TomatoesFieldGrownPerLb          Decimal? @db.Decimal(6, 3)
  ElectricityPerKWH                Decimal? @db.Decimal(6, 3)
  GasolineUnleadedRegularPerGallon Decimal? @db.Decimal(6, 3)
  UtilityPipedGasPerTherm          Decimal? @db.Decimal(6, 3)
}

model CRYPTO {
  Symbol      String   @db.VarChar(50)
  DateTime    DateTime @db.DateTime
  Price_Open  Decimal? @db.Decimal(18, 2)
  Price_Close Decimal? @db.Decimal(18, 2)
  Price_Low   Decimal? @db.Decimal(18, 2)
  Price_High  Decimal? @db.Decimal(18, 2)
  Volume      BigInt?

  @@id([Symbol, DateTime], map: "PK_CRYPTO_SPY")
}

model FOREX {
  Symbol   String   @db.VarChar(50)
  DateTime DateTime @db.DateTime
  Rate     Decimal? @db.Decimal(18, 2)

  @@id([Symbol, DateTime], map: "PK_FOREX_SPY")
}

model FRED_Consumer_Credit_Owned_And_Securitized {
  DateTime DateTime @id(map: "PK_FRED_Consumer_Credit_Owned_And_Securitized") @db.DateTime
  Value    Decimal? @db.Decimal(9, 4)
}

