}

model INDEX {
  Symbol      String   @db.VarChar(50)
  DateTime    DateTime @db.DateTime
  Price_Open  Decimal? @db.Decimal(18, 2)
  Price_Close Decimal? @db.Decimal(18, 2)
  Price_Low   Decimal? @db.Decimal(18, 2)
  Price_High  Decimal? @db.Decimal(18, 2)
  Volume      BigInt?

  @@id([Symbol, DateTime], map: "PK_INDEX_SPY")
}

model STOCK {
  Symbol      String   @db.VarChar(50)
  DateTime    DateTime @db.DateTime
  Price_Open  Decimal? @db.Decimal(18, 2)
  Price_Close Decimal? @db.Decimal(18, 2)
  Price_Low   Decimal? @db.Decimal(18, 2)
  Price_High  Decimal? @db.Decimal(18, 2)
  Volume      BigInt?

  @@id([Symbol, DateTime], map: "PK_STOCK_SPY")
}
